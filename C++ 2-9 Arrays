#include <iostream>
#include <Windows.h>
using namespace std;



int main() {

	string music[3];

	music[0] = "Its Golden Hour Somewhere: By Lovejoy ";
	music[1] = "Tangerine: By Glass Animals ";
	music[2] = "Eighth Wonder: By Lemon Demon ";

	for (int i = 0; i < 3; i++)
		cout << music[i] << " ";
	cout << endl;
	cout << "" << endl;

	music[1] = " Dear Arkansas Daughter: By Lady Lamb ";

	cout << music[2];
	cout << "" << endl;
	cout << "" << endl;


	for (int i = 0; i < 3; i++)
		cout << music[i] << " ";
	cout << endl;
	cout << "" << endl;

	for (int i = 0; i < 5; i++)
		if (music[i] == "Missus Piano: By Rio Romeo")
			cout << "Now playing Missus Piano: By Rio Romeo" << endl;
		else;
			cout << "Missus Piano: By Rio Romeo is not in the queue!" << endl;
}
