#include <iostream>
#include <Windows.h>
using namespace std;

int main() {

	for (int i = 0; i < 6; i++) {
		for (int j = 0; j < 10; j++)
			cout << "$";
		cout << endl;
	}
	cout << endl;

	for (int k = 3; k < 6; k++) {
		for (int l = 0; l < 5; l++)
			cout << k;
		cout << endl;
	}
	cout << endl;
}
